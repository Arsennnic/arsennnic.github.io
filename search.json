---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "subtitle": "{{ post.subtitle }}",
      "content": "{{ post.content | strip_html | strip_newlines }}",
      "tags": "{{ post.tags | join: ', ' }}",
      "url": "{{ site.url }}{{ post.url }}",
      "date": "{{ post.date | date: "%B %-d, %Y" }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]